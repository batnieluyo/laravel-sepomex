{
    "name": "wafto/laravel-sepomex",
    "description": "Servicio Postal Mexicano (Sepomex) laravel library",
    "keywords": ["laravel", "sepomex", "mexico"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/wafto/laravel-sepomex/issues",
        "source": "https://github.com/wafto/laravel-sepomex"
    },
    "type": "library",
    "author": {
        "name": "Aftab Hussain Miranda",
        "email": "wafto.mx@gmail.com"
    },
    "require": {
        "php": ">=7.0",
        "illuminate/console": "5.5.x",
        "illuminate/contracts": "5.5.x",
        "illuminate/database": "5.5.x",
        "illuminate/support": "5.5.x"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Aftab\\Sepomex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aftab\\Sepomex\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aftab\\Sepomex\\SepomexServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
