{
    "name": "wafto/laravel-sepomex",
    "description": "Servicio Postal Mexicano (Sepomex) laravel library",
    "keywords": ["laravel", "sepomex", "mexico"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/wafto/laravel-sepomex/issues",
        "source": "https://github.com/wafto/laravel-sepomex"
    },
    "type": "library",
    "authors": [
        {
            "name": "Aftab Hussain Miranda",
            "email": "wafto.mx@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "5.5.x|5.6.x|5.7.x|5.8.x|^6.0|^7.0",
        "illuminate/contracts": "5.5.x|5.6.x|5.7.x|5.8.x|^6.0|^7.0",
        "illuminate/database": "5.5.x|5.6.x|5.7.x|5.8.x|^6.0|^7.0",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x|^6.0|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*|^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Aftab\\Sepomex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aftab\\Sepomex\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aftab\\Sepomex\\SepomexServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
